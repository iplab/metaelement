<xml>
	<row>
		<#if (products?exists)>
			<#list products.row as product> 
				<#if (product.product_id?exists)>
					<id>${product.product_id}</id>
				</#if>
				<#if (product.ref_number?exists)>
					<ref_number>${product.ref_number}</ref_number>
				</#if>
				<#if (product.title?exists)>
					<title>${product.title}</title>
				</#if>
				<#if (product.short_description?exists)>
					<short_description>${product.short_description}</short_description>
				</#if>
				<#if (product.description?exists)>
					<description>${product.description}</description>
				</#if>
				<#if (product.bbc_catalog_ref?exists)>
					<bbc_catalog_ref>${product.bbc_catalog_ref}</bbc_catalog_ref>
				</#if>
				<#if (product.sku?exists)>
					<sku>${product.sku}</sku>
				</#if>
				<#if (product.active?exists)>
					<active>${product.active}</active>
				</#if>
				<#if (product.promotions?exists)>
					<promotions>${product.promotions}</promotions>
				</#if>
				<#if (product.product_canada?exists)>
					<product_canada>${product.product_canada}</product_canada>
				</#if>
				<#if (product.item_number?exists)>
					<item_number>${product.item_number}</item_number>
				</#if>
				<#if (product.msrp_us?exists)>
					<msrp_us>${product.msrp_us}</msrp_us>
				</#if>
				<#if (product.msrp_canada?exists)>
					<msrp_canada>${product.msrp_canada}</msrp_canada>
				</#if>
				<#if (product.our_price_us?exists)>
					<our_price_us>${product.our_price_us}</our_price_us>
				</#if>
				<#if (product.our_price_canada?exists)>
					<our_price_canada>${product.our_price_canada}</our_price_canada>
				</#if>
				<#if (product.sale_price_us?exists)>
					<sale_price_us>${product.sale_price_us}</sale_price_us>
				</#if>
				<#if (product.sale_price_canada?exists)>
					<sale_price_canada>${product.sale_price_canada}</sale_price_canada>
				</#if>
				<#if (product.drop_ship_us?exists)>
					<drop_ship_us>${product.drop_ship_us}</drop_ship_us>
				</#if>
				<#if (product.drop_ship_canada?exists)>
					<drop_ship_canada>${product.drop_ship_canada}</drop_ship_canada>
				</#if>
				<#if (product.availability_us?exists)>
					<availability_us>${product.availability_us}</availability_us>
				</#if>
				<#if (product.availability_date_us?exists)>
					<availability_date_us>${product.availability_date_us}</availability_date_us>
				</#if>
				<#if (product.availability_canada?exists)>
					<availability_canada>${product.availability_canada}</availability_canada>
				</#if>
				<#if (product.availability_date_canada?exists)>
					<availability_date_canada>${product.availability_date_canada}</availability_date_canada>
				</#if>
				<#if (product.release_date?exists)>
					<release_date>${product.release_date}</release_date>
				</#if>
				<#if (product.gift_wrap?exists)>
					<gift_wrap>${product.gift_wrap}</gift_wrap>
				</#if>
				<#if (product.manufacturer_number?exists)>
					<manufacturer_number>${product.manufacturer_number}</manufacturer_number>
				</#if>
				<#if (product.number_of_pages?exists)>
					<number_of_pages>${product.number_of_pages}</number_of_pages>
				</#if>
				<#if (product.runtime?exists)>
					<runtime>${product.runtime}</runtime>
				</#if>
				<#if (product.number_of_items?exists)>
					<number_of_items>${product.number_of_items}</number_of_items>
				</#if>
				<#if (product.number_of_tapes?exists)>
					<number_of_tapes>${product.number_of_tapes}</number_of_tapes>
				</#if>
				<#if (product.special_features?exists)>
					<special_features>${product.special_features}</special_features>
				</#if>
				<#if (product.plot_summary?exists)>
					<plot_summary>${product.plot_summary}</plot_summary>
				</#if>
				<#if (product.awards?exists)>
					<awards>${product.awards}</awards>
				</#if>
				<#if (product.fun_facts?exists)>
					<fun_facts>${product.fun_facts}</fun_facts>
				</#if>
				<#if (product.press_quotes?exists)>
					<press_quotes>${product.press_quotes}</press_quotes>
				</#if>
				<#if (product.close_captions?exists)>
					<close_captions>${product.close_captions}</close_captions>
				</#if>
				<#if (product.originally_aired?exists)>
					<originally_aired>${product.originally_aired}</originally_aired>
				</#if>
				<#if (product.abum_credits?exists)>
					<abum_credits>${product.abum_credits}</abum_credits>
				</#if>
				<#if (product.liner_notes?exists)>
					<liner_notes>${product.liner_notes}</liner_notes>
				</#if>
				<#if (product.dimensions?exists)>
					<dimensions>${product.dimensions}</dimensions>
				</#if>
				<#if (product.color?exists)>
					<color>${product.color}</color>
				</#if>
				<#if (product.personalized?exists)>
					<personalized>${product.personalized}</personalized>
				</#if>
				<#if (product.size?exists)>
					<size>${product.size}</size>
				</#if>
				<#if (product.delivery_frequency?exists)>
					<delivery_frequency>${product.delivery_frequency}</delivery_frequency>
				</#if>
				<#if (product.average_page_per_issue?exists)>
					<average_page_per_issue>${product.average_page_per_issue}</average_page_per_issue>
				</#if>
				<#if (product.regular_features?exists)>
					<regular_features>${product.regular_features}</regular_features>
				</#if>
				<#if (product.current_issue_content?exists)>
					<current_issue_content>${product.current_issue_content}</current_issue_content>
				</#if>
				<#if (product.back_order_date_us?exists)>
					<back_order_date_us>${product.back_order_date_us}</back_order_date_us>
				</#if>
				<#if (product.back_order_date_canada?exists)>
					<back_order_date_canada>${product.back_order_date_canada}</back_order_date_canada>
				</#if>
				<#if (product.episode_description?exists)>
					<episode_description>${product.episode_description}</episode_description>
				</#if>
				<#if (product.system_requirements?exists)>
					<system_requirements>${product.system_requirements}</system_requirements>
				</#if>
				<#if (product.simon_says?exists)>
					<simon_says>${product.simon_says}</simon_says>
				</#if>
				<#if (product.show_more_genre_id?exists)>
					<show_more_genre_id>${product.show_more_genre_id}</show_more_genre_id>
				</#if>
				<#if (product.show_more_count?exists)>
					<show_more_count>${product.show_more_count}</show_more_count>
				</#if>
				<#if (product.show_more_description?exists)>
					<show_more_description>${product.show_more_description}</show_more_description>
				</#if>
				<#if (product.weight?exists)>
					<weight>${product.weight}</weight>
				</#if>
				<#if (product.page_name?exists)>
					<page_name>${product.page_name}</page_name>
				</#if>
				<#if (product.create_date?exists)>
					<create_date>${product.create_date}</create_date>
				</#if>
				<#if (product.modify_date?exists)>
					<modify_date>${product.modify_date}</modify_date>
				</#if>
				<#if (product.main_image?exists)>
					<main_image>${product.main_image}</main_image>
				</#if>
				<#if (product.feature_image?exists)>
					<feature_image>${product.feature_image}</feature_image>
				</#if>
				<#if (product.browse_image?exists)>
					<browse_image>${product.browse_image}</browse_image>
				</#if>
				<#if (product.thumbnail_image?exists)>
					<thumbnail_image>${product.thumbnail_image}</thumbnail_image>
				</#if>
				<#if (product.src_image?exists)>
					<src_image>${product.src_image}</src_image>
				</#if>
				<#if (product.large_image?exists)>
					<large_image>${product.large_image}</large_image>
				</#if>
				<#if (product.drop_ship_details?exists)>
					<drop_ship_details>${product.drop_ship_details}</drop_ship_details>
				</#if>
				<#if (product.number_of_issues?exists)>
					<number_of_issues>${product.number_of_issues}</number_of_issues>
				</#if>
				<#if (product.format?exists)>
					<format>${product.format}</format>
				</#if>
				<#if (product.product_inventory?exists)>
					<product_inventory>${product.product_inventory}</product_inventory>
				</#if>
				<#if (product.type?exists)>
					<type>${product.type}</type>
				</#if>
				<#assign curModIndex = 0> 
				<#if (product.other_formats?exists)>
					<other_formats><![CDATA[
						<#list product.other_formats as formats>
							<#if (formats.name?exists) && (formats.link?exists)>
								<#if curModIndex = 0>  
									<a href="${formats.link}">${formats.name}</a>
									<#assign curModIndex = curModIndex+1> 
								<#else>
									,<a href="${formats.link}">${formats.name}</a>
									<#assign curModIndex = curModIndex+1> 
								</#if> 
							</#if> 
						</#list>
					]]> </other_formats>
				</#if>
				<#list product.genres as genre>
					<step>
						<#if (genre.name?exists)>
							<name>${genre.name}</name>
						</#if>			
						<#if (genre.id?exists)>
							<id>${genre.id}</id>
						</#if>
						<#if (genre.primary?exists)>
							<primary>${genre.primary}</primary>
						</#if>
					</step>
				</#list>
			</#list>
		</#if>
	</row>
</xml>